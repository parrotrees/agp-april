<link rel="stylesheet" href="https://widget.reviews.io/rating-snippet/dist.css" />

<script>
  var loadRating = function () {
    ratingSnippet("ruk_rating_snippet",{
    store: "box-fox",
    color: "#3a3a3a",
    linebreak: false,
    text: "Reviews",
      usePolaris: true,
    });
  }
  
      loadRating();

  
  $(document).on('click', '[data-secondary-action="open-quick-view"], [data-pagination]', function () {
    setTimeout(function(){
      loadRating();
    }, 500);
  });
    </script>
  <script>
jQuery(function() {
  jQuery('.swatch :radio').change(function() {
    var optionIndex = jQuery(this).closest('.swatch').attr('data-option-index');
    var optionValue = jQuery(this).val();
    jQuery(this)
      .closest('form')
      .find('.single-option-selector')
      .eq(optionIndex)
      .val(optionValue)
      .trigger('change');
  });
});
</script>

    <style>.ruk_rating_snippet { display: block !important } .ruk_rating_snippet span { font-family: inherit !Important } </style>
    

    
